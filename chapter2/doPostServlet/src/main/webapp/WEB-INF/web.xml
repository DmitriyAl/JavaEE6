<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <servlet>
        <servlet-name>FormHandlerServlet</servlet-name>
        <servlet-class>FormHandlerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormHandlerServlet</servlet-name>
        <url-pattern>/example</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MultipleValueFieldHandlerServlet</servlet-name>
        <servlet-class>MultipleValueFieldHandlerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MultipleValueFieldHandlerServlet</servlet-name>
        <url-pattern>/multipleValueFieldHandlerServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>RedirectedMultipleValueHandlerServlet</servlet-name>
        <servlet-class>RedirectedMultipleValueHandlerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RedirectedMultipleValueHandlerServlet</servlet-name>
        <url-pattern>/redirected</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ConfirmationServlet</servlet-name>
        <servlet-class>ConfirmationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConfirmationServlet</servlet-name>
        <url-pattern>/confServlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>smth.html</welcome-file>
    </welcome-file-list>

</web-app>
